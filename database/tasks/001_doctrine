<?php
// src/User.php
/**
 * @Entity @Table(name="users")
 */
class User
{
    /**
     * @Id @GeneratedValue @Column(type="integer")
     * @var int
     */
    protected $id;
    /**
     * @Column(type="string")
     * @var string
     */
    protected $firstname;

    public function getId()
    {
        return $this->id;
    }

    public function getFirstName()
    {
        return $this->firstname;
    }

    public function setFirstName($name)
    {
        $this->firstname = $firstname;
    }
}



Step 2. http://www.doctrine-project.org/about.html

<?php

$user = new User();
$user->setId(1);
$user->setFirstname('Andreas');
TODO: Alle set-Methode (Insert) und get_methoden (Selekt) aufrufen

$om = $this->getYourObjectManager();
$om->persist($user);
$om->flush(); // insert the new document

